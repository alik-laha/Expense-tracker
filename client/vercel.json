{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}